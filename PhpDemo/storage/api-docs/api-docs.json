{
    "swagger": "2.0",
    "info": {
        "title": "Demo API",
        "contact": {
            "name": "Dave van Herten",
            "url": "http://dave.vanherten.ca"
        },
        "version": "1.0"
    },
    "host": "localhost:8000",
    "basePath": "",
    "schemes": [
        "http"
    ],
    "paths": {
        "/api/demo": {
            "get": {
                "tags": [
                    "demo"
                ],
                "summary": "Display a listing of our demo items.",
                "description": "Returns demo items.",
                "operationId": "api.demo.index",
                "produces": [
                    "application/json"
                ],
                "responses": {
                    "200": {
                        "description": "Demo array."
                    }
                }
            }
        },
        "/api/demo/{id}": {
            "get": {
                "tags": [
                    "demo"
                ],
                "summary": "Display a single demo item.",
                "description": "Returns demo overview.",
                "operationId": "api.demo.index",
                "produces": [
                    "application/json"
                ],
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The id of the demo item to retrieve",
                        "required": true,
                        "type": "integer"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "A Demo item."
                    },
                    "404": {
                        "description": "Demo item missing."
                    }
                }
            }
        }
    },
    "definitions": {
        "Error": {
            "required": [
                "code",
                "message"
            ],
            "properties": {
                "code": {
                    "description": "Class Controller",
                    "type": "integer",
                    "format": "int32"
                },
                "message": {
                    "description": "Class Controller",
                    "type": "string"
                }
            }
        }
    }
}